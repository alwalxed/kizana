{
  "name": "kizana",
  "version": "0.0.0",
  "main": "kizana_resources/index.js",
  "description": "خزانة الكتب فيها ثمانية آلاف كتاب للقراءة و البحث فيها",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "license": "GPL-3.0-or-later",
  "build": {
    "appId": "com.kizana.app",
    "productName": "الخزانة",
    "files": ["!kizana_resources/kizana_all_books.sqlite" ,"!kizana_resources/index.sqlite"],
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "linux": {
      "category": "Education",
      "desktop": {
        "Comment": "خزانة الكتب فيها ثمانية آلاف كتاب للقراءة و البحث فيها",
        "Name": "الخزانة",
        "Type": "Application",
        "Categories": "Education"
      },
      "executableName": "kizana",
      "maintainer": "kizana@gmail.com",
      "asar": "false",
      "icon": "build/icon.icns",
      "target": ["AppImage"]
    },
    "mac": {
      "category": "public.app-category.tools",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "chrome-tabs": "^5.4.0",
    "jquery": "3.6.0",
    "knex": "0.95.05",
    "oktob.js": "^1.1.0",
    "path": "0.12.7",
    "sqlite3": "5.0.0"
  },
  "devDependencies": {
    "electron": "13.1.5",
    "electron-builder": "^22.11.7",
    "electron-reloader": "1.1.0"
  }
}
